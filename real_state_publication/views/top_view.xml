<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
      
      <record model="ir.ui.view" id="real_state_top_press_form">
          <field name="name">real.state.top.press.form</field>
          <field name="model">real.state.top.press</field>
          <field name="type">form</field>
          <field name="arch" type="xml">
              <field name="name" />
              <field name="actual" />
              <field name="top_id" />
              <field name="date_publications" colspan="4" widget="one2many" />
          </field>
      </record>
      
      <record model="ir.ui.view" id="real_state_top_form_publication">
          <field name="name">real.state.top.form.publication</field>
          <field name="model">real.state.top</field>
          <field name="type">form</field>
          <field name="inherit_id" ref="real_state.real_state_top_form"/>
          <field name="arch" type="xml">
              <page string="Images and documents" position="before">
                  <page string="Publications">
                      <group colspan="2" col="4">
                          <separator string="Internet" colspan="4" />
                           <field name="internet_wo" nolabel="1" colspan="4">
                              <form string="Without Automatic Update">
                                  <field name="name" />
                                  <field name="actual" />
                              </form>
                              <tree string="Without Automatic Update" editable="bottom">
                                  <field name="name"/>
                                  <field name="actual" />
                              </tree>
                          </field> 
                          <separator string="Internet Description" colspan="4" />
                          <field name="internet_description" nolabel="1" colspan="4" />
                      </group>
                      <group colspan="2" col="4">
                     <!--  <separator string="Press" colspan="4" />
                           <field name="publications" nolabel="1" colspan="4">
                              <form string="Publications">
                              	<group>
                                  <field name="name" />
                                  <field name="actual" />
                                  <field name="date_publications" colspan="4" widget="one2many" />
                              	</group>
                              </form>
                              <tree string="Publications">
                                  <field name="name"/>
                                  <field name="actual" />
                                  <field name="date_publications" />
                              </tree>
                          </field> -->        
                          <separator string="In the top" colspan="4" />
                          <field name="poster" />
                      </group>
                  </page>
              </page>
              
              <!--<field name="energy_doc" position="after">
              		<field name="energy_doc_url" widget="url" readonly="1"/>
              </field>   -->
          </field>
      </record>
      
      <record model="ir.ui.view" id="real_state_top_filter_publication">
          <field name="name">real.state.top.filter.publication</field>
          <field name="model">real.state.top</field>
          <field name="type">search</field>
          <field name="inherit_id" ref="real_state.real_state_top_filter"/>
          <field name="arch" type="xml">
          		<field name="partner_id" position="after">
          			<field name="publications" />
          			<field name="internet_wo" /><newline />
          		</field>
          </field>
      </record>

