<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
      
      <record model="ir.ui.view" id="real_estate_top_form_publication_common">
          <field name="name">real.estate.top.form.publication.common</field>
          <field name="model">real.estate.top</field>
          <field name="inherit_id" ref="real_estate_publication.real_estate_top_form_publication"/>
          <field name="arch" type="xml">
            <!--  <page string="Publications" position="after">
                    <page string="Portals and Website">
                        <separator string="Common information" colspan="4" />
                        <group>
                        	<field name="latitude" />
                        	<field name="longitude" />
                        	<field name="top_state" />
                        	<field name="door" />
                        </group>
                    </page>
                </page>   -->  
                
                <xpath expr="//page[5]" position="after">
                	<page string="Portals and Website">
                        <separator string="Common information" colspan="4" />
                        <group>
                        	<field name="latitude" />
                        	<field name="longitude" />
                        	<field name="google_maps_url" widget="url"/>
                        	<field name="openstreetmap_url" widget="url"/>
                        	<field name="top_state" />
                        	<field name="door" />
                        </group>
                    </page>
                </xpath>
          </field>
      </record>

      <record model="ir.ui.view" id="real_estate_top_filter_common">
          <field name="name">real.estate.top.filter.common</field>
          <field name="model">real.estate.top</field>
          <field name="inherit_id" ref="real_estate_base.real_estate_top_filter"/>
          <field name="arch" type="xml">
             <group name="price-area" position="after">
                <group string="Internet" name="internet"/>
             </group>
          </field>
      </record>

